# Azure Vision API
VISION_ENDPOINT=https://your-vision-service.cognitiveservices.azure.com/
VISION_KEY=your_vision_key

# Azure Form Recognizer
FORM_RECOGNIZER_ENDPOINT=https://your-form-recognizer.cognitiveservices.azure.com/
FORM_RECOGNIZER_KEY=your_form_recognizer_key

# Azure Text Analytics
TEXT_ANALYTICS_ENDPOINT=https://your-text-analytics.cognitiveservices.azure.com/
TEXT_ANALYTICS_KEY=your_text_analytics_key

# Azure Speech Services
SPEECH_KEY=your_speech_key
SPEECH_REGION=your_region

# Azure Content Safety
CONTENT_SAFETY_ENDPOINT=https://your-content-safety.cognitiveservices.azure.com/
CONTENT_SAFETY_KEY=your_content_safety_key

# Azure Document Translation
DOCUMENT_TRANSLATION_ENDPOINT=https://your-translation-service.cognitiveservices.azure.com/
DOCUMENT_TRANSLATION_KEY=your_translation_key

# Azure Storage
STORAGE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=your_account_name;AccountKey=your_account_key;EndpointSuffix=core.windows.net
STORAGE_CONTAINER=documents

# Azure Cosmos DB
COSMOS_ENDPOINT=https://your-cosmos-account.documents.azure.com:443/
COSMOS_KEY=your_cosmos_key
COSMOS_DATABASE=your_database_name
COSMOS_CONTAINER=your_container_name

# Application Settings
DEBUG=True
PORT=8000
ALLOWED_ORIGINS=http://localhost:3000 


